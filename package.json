{
  "name": "ioc_template",
  "module": "./src/index.ts",
  "type": "module",
  "scripts": {
    "start": "cross-env NODE_ENV=production bun run index.ts",
    "dev": "cross-env NODE_ENV=development bun --watch run index.ts ",
    "build": "bun build ./src/index.ts --outdir ./dist",
    "compile": "bun build --bytecode --target=bun --compile --sourcemap index.ts "
  },
  "devDependencies": {
    "@types/bun": "latest"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "@types/node": "^22.7.5",
    "axios": "^1.7.7",
    "ccxt": "^4.4.17",
    "chalk": "^5.3.0",
    "console-table-printer": "^2.12.1",
    "cross-env": "^7.0.3",
    "discord.js": "^14.16.3",
    "elysia": "^1.1.23",
    "express": "^4.19.2",
    "inversify": "^6.0.2",
    "ioredis": "^5.4.1",
    "mongoose": "^8.7.1",
    "reflect-metadata": "^0.2.2",
    "zod": "^3.23.8"
  }
}